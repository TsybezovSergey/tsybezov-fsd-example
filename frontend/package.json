{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prestart": "npm run orval",
    "dev": "npm run prestart && next dev -p 4000",
    "build": "next build",
    "start": "next start -p 4000",
    "lint": "eslint",
    "delete:generated": "rimraf ./orval/app/api-generated",
    "orval": "npm run delete:generated && orval --config ./orval/config.ts"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.6",
    "next": "16.0.1",
    "orval": "^7.16.0",
    "prettier": "^3.6.2",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "sass": "^1.93.3",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "typescript": "^5"
  }
}
